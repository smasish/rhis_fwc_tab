<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_deliveryDetails"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/fragmentborder"
        android:weightSum="1"
        android:orientation="vertical">

        <LinearLayout
            style="@style/rowHeading"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingBottom="10sp"
            android:paddingTop="10sp">

            <TextView
                android:id="@+id/TextViewClient_info_tab"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:text="@string/delevery_related_info"
                android:textColor="#FFFFFF"
                android:textSize="20sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/delivery_place"
                android:layout_width="134dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:text="@string/delivery_place"
                android:textSize="20dp" />

            <Spinner
                android:id="@+id/delivery_placeDropdown"
                android:layout_width="101dp"
                android:layout_height="60px"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:entries="@array/FacilityPlace_DropDown"
                android:textSize="20dp"
                android:prompt="@string/delivery_place"/>

            <TextView
                android:id="@+id/delivery_type"
                android:layout_width="120dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:text="@string/delivery_type"
                android:textSize="20dp" />

            <Spinner
                android:id="@+id/delivery_typeDropdown"
                android:layout_width="100dp"
                android:layout_height="60px"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:textSize="20dp"
                android:entries="@array/DeliveryType_DropDown"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/TextViewdelivery_date"
                android:layout_width="140dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:text="@string/delivery_date"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/id_delivery_date"
                android:layout_width="120dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:paddingLeft="10dp"
                android:background="@drawable/edittextstyle"
                android:inputType="date"
                android:hint="dd/mm/yyyy"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageViewDeliveryDate"
                android:src="@drawable/calendar_icon"
                android:layout_marginTop="5dp"
                android:onClick="pickDate"/>

            <TextView
                android:id="@+id/TaxtViewdelivery_time"
                android:layout_width="50dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:text="@string/delivery_time"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/delivery_time_hour"
                android:layout_width="40dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:background="@drawable/edittextstyle"
                android:inputType="number" />

            <TextView
                android:id="@+id/TaxtViewdelivery_time_colon"
                android:layout_width="10dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:text=":"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/delivery_time_minute"
                android:layout_width="40dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:background="@drawable/edittextstyle"
                android:inputType="number" />

            <Spinner
                android:id="@+id/delivery_time_Dropdown"
                android:layout_width="50dp"
                android:layout_height="60px"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/id_facililties_section_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/TaxtViewdscn"
                android:layout_width="wrap_content"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:text="@string/service_center_name"
                android:textSize="20dp" />

            <Spinner
                android:id="@+id/id_facility_name_Dropdown"
                android:layout_width="200dp"
                android:layout_height="60px"
                android:layout_marginLeft="10dp"
                android:entries="@array/FacilityType_DropDown"
                android:layout_marginTop="10dp"
                android:textSize="20dp"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/id_facililties_admission_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:text="@string/admission_date"
                android:textSize="20dp" />
            <EditText
                android:id="@+id/id_admissionDate"
                android:layout_width="120dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:paddingLeft="10dp"
                android:background="@drawable/edittextstyle"
                android:inputType="date"
                android:hint="dd/mm/yyyy"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageViewAdmissionDate"
                android:src="@drawable/calendar_icon"
                android:layout_marginTop="5dp"
                android:onClick="pickDate"/>

            <TextView
                android:layout_width="50dp"
                android:layout_height="50px"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/ward"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/id_ward"
                android:layout_width="40dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edittextstyle"/>
            <TextView
                android:layout_width="50dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:text="@string/bed"
                android:textSize="20dp" />
            <EditText
                android:id="@+id/id_bed"
                android:layout_width="40dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edittextstyle"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/id_deliveryResultLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="10dp">

            <TextView
                android:id="@+id/TaxtViewdelivery_result"
                android:layout_width="160dp"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:text="@string/delivery_result"
                android:textSize="20dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:orientation="horizontal"
                android:saveEnabled="false">

                <TextView
                    android:id="@+id/TaxtViewLive_born"
                    android:layout_width="110dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/live_born"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/Live_born"
                    android:layout_width="40dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/edittextstyle"
                    android:inputType="number" />

                <TextView
                    android:id="@+id/TaxtViewDead_born"
                    android:layout_width="80dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:text="@string/dead_born"
                    android:textSize="20dp" />
                <EditText
                    android:id="@+id/Dead_born"
                    android:layout_width="40dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/edittextstyle"
                    android:inputType="number" />

                <TextView
                    android:id="@+id/TaxtViewDead_born_fresh"
                    android:layout_width="60dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:text="@string/dead_born_fresh"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/Dead_born_fresh"
                    android:layout_width="40dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/edittextstyle"
                    android:inputType="number" />

                <TextView
                    android:id="@+id/TaxtViewDead_born_macerated"
                    android:layout_width="100dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:text="@string/dead_born_macerated"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/Dead_born_macerated"
                    android:layout_width="40dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/edittextstyle"
                    android:inputType="number" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/taxtChildSex"
                    android:layout_width="50dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/child_sex"
                    android:textSize="20dp" />

                <TextView
                    android:id="@+id/textSon"
                    android:layout_width="50dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/Client_Son_Num"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/son"
                    android:layout_width="40dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/edittextstyle"
                    android:inputType="number" />

                <TextView
                    android:id="@+id/textDaughter"
                    android:layout_width="50dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:text="@string/Client_Daughter_Num"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/daughter"
                    android:layout_width="40dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/edittextstyle"
                    android:inputType="number" />
                <TextView
                    android:id="@+id/textNotDetected"
                    android:layout_width="160dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:text="@string/not_detected"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/notDetected"
                    android:layout_width="40dp"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/edittextstyle"
                    android:inputType="number" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/id_DeliveryManagementLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginTop="10dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textamtsl"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:text="@string/amtsl"
                android:textSize="20dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:orientation="vertical">

                <CheckBox
                    android:id="@+id/oxytocin"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/oxytocin"
                    android:textSize="20dp" />

                <CheckBox
                    android:id="@+id/controlChordTraction"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/traction"
                    android:textSize="20dp" />

                <CheckBox
                    android:id="@+id/uterusMassage"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/massage"
                    android:textSize="20dp" />
            </LinearLayout>


            <LinearLayout
                android:id = "@+id/id_Epcotomi_section_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/idEpcotomi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/epcotomi"
                    android:textSize="20dp" />
                <RadioGroup
                    android:id="@+id/id_radioGroupEpctiomi"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <RadioButton
                    android:id="@+id/radioEpc_yes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/general_yes"
                    android:textSize="20dp" />

                <RadioButton
                    android:id="@+id/radioEpc_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/general_no"
                    android:textSize="20dp" />
                </RadioGroup>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/idOxytocin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/oxytocin"
                    android:textSize="20dp" />
                <RadioGroup
                    android:id="@+id/id_radioGroupOytocin"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <RadioButton
                    android:id="@+id/radioOxytocin_yes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/general_yes"
                    android:textSize="20dp" />

                <RadioButton
                    android:id="@+id/radioOxytocin_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/general_no"
                    android:textSize="20dp" />
                </RadioGroup>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/idCOmplication"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/deliveryComplication"
                android:textSize="20dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/id_delayedDelivery"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/delayedDelivery"
                    android:textSize="20dp" />

                <CheckBox
                    android:id="@+id/idBloodLoss"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/bloodLoss"
                    android:textSize="20dp" />

                <CheckBox
                    android:id="@+id/id_blockedDelivery"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/blockedDelivery"
                    android:textSize="20dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/id_blockedPlacenta"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/blockedPlacenta"
                    android:textSize="20dp" />

                <CheckBox
                    android:id="@+id/id_heavyHedache"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/heavyHedache"
                    android:textSize="20dp" />

                <CheckBox
                    android:id="@+id/id_blurryVision"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/blurryVision"
                    android:textSize="20dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/id_onlyHead"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/onlyHead"
                    android:textSize="20dp" />

                <CheckBox
                    android:id="@+id/id_convulsion"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/convulsion"
                    android:textSize="20dp" />

                <CheckBox
                    android:id="@+id/id_deleiveryExtra"
                    android:layout_width="wrap_content"
                    android:layout_height="50px"
                    android:layout_marginTop="10dp"
                    android:text="@string/deleiveryExtra"
                    android:textSize="20dp" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/id_general_treatment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/general_treatment"
                android:textSize="20dp" />

            <Spinner
                android:id="@+id/id_treatment_spinner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/id_general_advice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/general_advice"
                android:textSize="20dp" />

            <Spinner
                android:id="@+id/id_spinner_advice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_weight="1" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/id_delivery_refer"
                android:layout_width="wrap_content"
                android:layout_height="50px"
                android:layout_marginTop="10dp"
                android:text="@string/general_refer"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/id_facility_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/general_facility_name"
                android:textSize="20dp" />

            <Spinner
                android:id="@+id/id_spinner_facilities"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:entries="@array/FacilityType_DropDown"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/id_delivery_cause"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/general_cause"
                android:textSize="20dp" />

            <Spinner
                android:id="@+id/id_spinner_delivery_cause"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center">
            <Button
                android:text="Save"
                android:layout_width="wrap_content"
                android:layout_height="80px"
                android:id="@+id/id_saveDelivryButton"
                android:onClick="onClick"/>
            <Button
                android:text="Edit"
                android:layout_width="wrap_content"
                android:layout_height="80px"
                android:id="@+id/id_editDelivryButton"
                android:onClick="onClick"/>

        </LinearLayout>
    <LinearLayout
        style="@style/rowHeading"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="10sp"
        android:paddingTop="10sp">
        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/newborn_related_info"
            android:id="@+id/deleveryLabel"
            android:gravity="center"
            android:layout_centerVertical="true"
            />
    </LinearLayout>
        <LinearLayout
            android:id="@+id/NewBorn" android:layout_marginLeft="10dp" android:layout_marginTop="10dp" android:paddingBottom="20dp" android:orientation="vertical" android:layout_centerVertical="true" android:layout_width="fill_parent" android:layout_height="wrap_content" android:background="#FFFFFF">
            <LinearLayout android:id="@+id/deliveryNewBornCondition" android:layout_marginTop="10dp"  android:orientation="horizontal" android:layout_centerVertical="true" android:layout_width="fill_parent" android:layout_height="wrap_content" >
                <TextView android:id="@+id/deliveryNewBornLabel" android:text="@string/child" android:textSize="20dp" android:layout_width="60dp" android:layout_height="50px" android:layout_marginTop="10dp" />
                <EditText android:id="@+id/deliveryNewBornNo." android:inputType="number" android:textSize="20dp" android:layout_width="40dp" android:layout_height="50px" android:layout_marginTop="10dp" android:background="@drawable/edittextstyle"/>
                <EditText android:id="@+id/deliveryNewBornConditionValue" android:inputType="text" android:textSize="20dp" android:layout_width="90dp" android:layout_height="50px" android:layout_marginTop="10dp" android:background="@drawable/edittextstyle"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/deliveryNewBorn" android:layout_marginTop="10dp"  android:orientation="horizontal" android:layout_centerVertical="true" android:layout_width="fill_parent" android:layout_height="wrap_content" >
                <RadioButton android:id="@+id/deliveryNewBornSon" android:text="@string/son" android:textSize="20dp" android:layout_width="80dp" android:layout_height="50px" />
